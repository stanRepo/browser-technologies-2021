  <label for="ratingCourseExplanation10">10: <input id="ratingCourseExplanation10"  type="radio" name="ratingExplanation" class="ratingCourseCheckbox" value="10" required></input></label>

</fieldset>

<label for="ratingUnderstanding"
>Hoe goed begrijp je de lesstof nu?</label>
<fieldset class="radioInputFieldset">
  <label for="ratingCourseUnderstanding1">1: <input id="ratingCourseUnderstanding1"type="radio" name="ratingUnderstanding" class="ratingCourseCheckbox" value="1" required></input></label>
  <label for="ratingCourseUnderstanding2">2: <input id="ratingCourseUnderstanding2"type="radio" name="ratingUnderstanding" class="ratingCourseCheckbox" value="2" required></input></label>
  <label for="ratingCourseUnderstanding3">3: <input id="ratingCourseUnderstanding3"type="radio" name="ratingUnderstanding" class="ratingCourseCheckbox" value="3" required></input></label>
  <label for="ratingCourseUnderstanding4">4: <input id="ratingCourseUnderstanding4"type="radio" name="ratingUnderstanding" class="ratingCourseCheckbox" value="4" required></input></label>
  <label for="ratingCourseUnderstanding5">5: <input id="ratingCourseUnderstanding5"type="radio" name="ratingUnderstanding" class="ratingCourseCheckbox" value="5" required></input></label>
  <label for="ratingCourseUnderstanding6">6: <input id="ratingCourseUnderstanding6"type="radio" name="ratingUnderstanding" class="ratingCourseCheckbox" value="6" required></input></label>
  <label for="ratingCourseUnderstanding7">7: <input id="ratingCourseUnderstanding7"type="radio" name="ratingUnderstanding" class="ratingCourseCheckbox" value="7" required></input></label>
  <label for="ratingCourseUnderstanding8">8: <input id="ratingCourseUnderstanding8"type="radio" name="ratingUnderstanding" class="ratingCourseCheckbox" value="8" required></input></label>
  <label for="ratingCourseUnderstanding9">9: <input id="ratingCourseUnderstanding9"type="radio" name="ratingUnderstanding" class="ratingCourseCheckbox" value="9" required></input></label>
  <label for="ratingCourseUnderstanding10">10: <input id="ratingCourseUnderstanding10"type="radio" name="ratingUnderstanding" class="ratingCourseCheckbox" value="10" required></input></label>

</fieldset>

      <label for="comments"
      >Is er iets wat je graag wilt vertellen?</label>
      <textarea
      form="enquete"
      name="feedback"
      id="comments"
      cols="30"
      rows="5"
      placeholder="Schrijf hier je feedback voor dit vak."
      ></textarea>
      <!-- Previous form items to be send along hidden from view -->
      <input type="text" value="<%= obj.firstName%>" name="firstName" hidden />
      <input type="text" value="<%= obj.lastName%>" name="lastName" hidden />
      <input
      type="text"
      value="<%= obj.studentNumber%>"
      name="studentNumber"
        hidden
        />
        <input
        type="text"
        value="<%= obj.dateOfBirth%>"
        name="dateOfBirth"
        hidden
        />
        <input
        class="inputDateNumberElements"
        type="number"
        name="dd"
        placeholder="dd"
        value="<%= obj.dd%>"
        hidden
      />
      <input
        class="inputDateNumberElements"
        type="number"
        name="mm"
        placeholder="mm"
        value="<%= obj.mm%>"
        hidden
      />
      <input
        class="inputDateNumberElements"
        type="number"
        name="yyyy"
        placeholder="yyyy"
        value="<%= obj.yyyy%>"
        hidden
      />
        
        <button id="submitEnqueteButton"type="submit">Versturen</button>
      </form>
    </section>
      
      <div id="progressBarContainer">
        <var id="progressBar">
        </var>
      </div>
      
<section class="offlineBannerSection"> 

</section>

      <script src="./javascripts/main.js" type="module"></script>
    </body>
    </html>
    