  <label for="ratingCourseExplanation4">4: <input id="ratingCourseExplanation4"  type="radio" name="ratingExplanation" class="ratingCourseCheckbox" value="4" required></input></label>
  <label for="ratingCourseExplanation5">5: <input id="ratingCourseExplanation5" type="radio" name="ratingExplanation" class="ratingCourseCheckbox" value="5" required></input></label>
  <label for="ratingCourseExplanation6">6: <input id="ratingCourseExplanation6" type="radio" name="ratingExplanation" class="ratingCourseCheckbox" value="6" required></input></label>
  <label for="ratingCourseExplanation7">7: <input id="ratingCourseExplanation7" type="radio" name="ratingExplanation" class="ratingCourseCheckbox" value="7" required></input></label>
